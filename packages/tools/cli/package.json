{
  "name": "@graphql-modules/cli",
  "version": "0.7.15",
  "repository": "https://github.com/Urigo/graphql-modules.git",
  "homepage": "https://graphql-modules.com/",
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts",
    "bin": {
      "graphql-modules": {
        "input": "./src/index.ts"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "graphql-modules": "dist/index.js"
  },
  "scripts": {
    "build": "bob",
    "prepack": "bob-update-version"
  },
  "dependencies": {
    "@graphql-modules/doctor": "0.7.15",
    "chalk": "4.0.0",
    "commander": "5.0.0",
    "log-symbols": "3.0.0"
  },
  "devDependencies": {
    "@types/node": "12.12.35",
    "bob-the-bundler": "0.3.4",
    "typescript": "3.8.3"
  }
}
